@inherits Umbraco.Web.Mvc.UmbracoViewPage<ContentModels.Simple>
@using ContentModels = CodeBase.Models;
@{
    Layout = "Site.cshtml";
}

<div id="grid"></div>
<script>
$("#grid").kendoGrid({
    selectable: "multiple cell",
    allowCopy: true,
    columns: [
        { field: "productName" },
        { field: "category" }
    ],
    dataSource: [
        { productName: "Tea", category: "Beverages" },
        { productName: "Coffee", category: "Beverages" },
        { productName: "Ham", category: "Food" },
        { productName: "Bread", category: "Food" }
    ]
});
</script>